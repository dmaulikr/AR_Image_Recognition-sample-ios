#import "OverlayViewController.h"


@implementation OverlayViewController


// Allow the application to start only in landscape right orientation
- (BOOL)shouldAutorotateToInterfaceOrientation:(UIInterfaceOrientation)interfaceOrientation
{
    BOOL ret;
    
    // Support both landscape orientations
    if (UIInterfaceOrientationLandscapeRight == interfaceOrientation ||
        UIInterfaceOrientationLandscapeLeft == interfaceOrientation) {
        ret = YES;
    }
    else {
        ret = NO;
    }
    
    return ret;
}

@end
